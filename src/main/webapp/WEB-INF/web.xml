<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SendMessageServlet</servlet-name>
  	<display-name>SendMessageServlet</display-name>
  	<description></description>
  	<servlet-class>SendMessageServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ContactServlet</servlet-name>
  	<display-name>ContactServlet</display-name>
  	<description></description>
  	<servlet-class>ContactServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ContactServlets</servlet-name>
  	<display-name>ContactServlets</display-name>
  	<description></description>
  	<servlet-class>com.example.ContactServlets</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SendMessageServlet</servlet-name>
  	<url-pattern>/SendMessageServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ContactServlet</servlet-name>
  	<url-pattern>/ContactServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ContactServlets</servlet-name>
  	<url-pattern>/ContactServlets</url-pattern>
  </servlet-mapping>
</web-app>
